#Copier-coller de notre mini Pipeline
node {
    stage('Clone') {
       git 'https://github.com/Ismabayili/jenkins-helloworld.git'   
     }
     stage('Build') {
         sh label: '', script: 'javac Main.java'    
     }
     stage('Run') {
         sh label: '', script: 'java Main'        
     }
 }
